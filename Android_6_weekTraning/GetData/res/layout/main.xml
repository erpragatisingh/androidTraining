<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent">
	<EditText android:id="@+id/privateId" android:layout_width="fill_parent"
		android:layout_height="wrap_content" />
	<EditText android:id="@+id/read" android:layout_width="fill_parent"
		android:layout_height="wrap_content" />
	<EditText android:id="@+id/write" android:layout_width="fill_parent"
		android:layout_height="wrap_content" />
	<EditText android:id="@+id/read_write" android:layout_width="fill_parent"
		android:layout_height="wrap_content" />
	<Button android:id="@+id/btn" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:text="GetDta" />
</LinearLayout>
